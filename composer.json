{
    "name": "krak/symfony-playground",
    "description": "Symfony playground for tinkering/testign your app.",
    "type": "symfony-bundle",
    "homepage": "https://github.com/krakphp/symfony-playground",
    "keywords": [
        "symfony",
        "symfony-bundle",
        "symfony4",
        "playground",
        "tinker"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "RJ Garcia",
            "email": "ragboyjr@icloud.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krak\\SymfonyPlayground\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krak\\SymfonyPlayground\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "nyholm/symfony-bundle-test": "^1.6",
        "phpunit/phpunit": "^9.1",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/var-dumper": "^5.1"
    },
    "scripts": {
        "test": "phpunit --testdox --colors=always"
    }
}
